rule CRISPR:
    '''Predict CRISPR with minced.'''
    input:
        fna = config['seq']
    output:
        '%s.gff' % tool_output
    params:
        tool_config['params']
    log:
        '%s.log' % tool_output
    priority:
        tool_config['priority']
    shell:
        'minced {params} -gff {input.fna} {output[0]} &> {log}'
